# This file is managed by puppet class <%= @name %>

<% @config_options.sort.map do |k,v| -%>
<%   if v.is_a?(Hash) -%>
[<%=   k %>]
<%     v.sort.map do |ki, vi| -%>
<%       if vi.is_a?(Array) -%>
<%=        ki %> = '<% vi.each do |vii| -%><%= vii %>:<% end -%>'
<%       elsif vi != :undef -%>
<%=        ki %> = <%= vi %>
<%       end -%>
<%     end -%>
<%   end %>
<% end -%>

# END
